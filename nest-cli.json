{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/trt-shop-backend/src",
  "monorepo": true,
  "root": "apps/trt-shop-backend",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/trt-shop-backend/tsconfig.app.json"
  },
  "projects": {
    "default": {
      "type": "application",
      "root": "apps/default",
      "entryFile": "main",
      "sourceRoot": "apps/default/src",
      "compilerOptions": {
        "tsConfigPath": "apps/default/tsconfig.app.json"
      }
    },
    "api-categories": {
      "type": "application",
      "root": "apps/api-categories",
      "entryFile": "main",
      "sourceRoot": "apps/api-categories/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-categories/tsconfig.app.json"
      }
    }
  }
}